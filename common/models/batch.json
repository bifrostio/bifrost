{
  "name": "batch",
  "description": "捐贈者一次捐贈行為所要送出去的物資資訊",
  "plural": "batches",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "createdDate": {
      "type": "date",
      "required": true
    },
    "updatedDate": {
      "type": "date"
    },
    "trackingNumber": {
      "type": "string",
      "required": true
    },
    "note": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "station": {
      "type": "belongsTo",
      "model": "station"
    },
    "provisionActivities": {
      "type": "hasMany",
      "model": "provisionActivity"
    },
    "contact": {
      "type": "embedsOne",
      "model": "contact",
      "property": "contact"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
