{
  "name": "batch",
  "description": "捐贈者一次捐贈行為所要送出去的物資資訊",
  "plural": "batches",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "createdDate": {
      "type": "date",
      "required": true
    },
    "updatedDate": {
      "type": "date"
    },
    "trackingNumber": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "station": {
      "type": "belongsTo",
      "model": "station"
    },
    "provisions": {
      "type": "hasMany",
      "model": "provision",
      "through": "provisionQuantity"
    },
    "contact": {
      "type": "embedsOne",
      "model": "contact",
      "property": "contact"
    }
  },
  "acls": [],
  "methods": {}
}
