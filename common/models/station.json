{
  "name": "station",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "batches": {
      "type": "hasMany",
      "model": "batch"
    },
    "contacts": {
      "type": "embedsMany",
      "model": "contact",
      "property": "contacts",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    },
    "project": {
      "type": "belongsTo",
      "model": "project"
    },
    "provisionRequirements": {
      "type": "hasMany",
      "model": "provisionRequirement"
    },
    "provisionActivities": {
      "type": "hasMany",
      "model": "provisionActivity"
    }
  },
  "acls": [],
  "methods": {}
}
